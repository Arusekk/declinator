(?imux)
^

(?P<title>
	(?:
		(?:
			mrs?|miss|pani?|mgr|dr
			|(?:
				x|ks|prof|inż|hab|św
			)\.?
		)
	\s)*
)

(?P<first>
	[^\W\d_]+
	(?:\s
		[^\W\d_]{4,}
	)*?
)

(?:
	(?P<inter>
		(?:\s
			[^\W\d_]{2,3}
		)*
	)

	(?P<last>
		\s[^\W\d_]+
		(?:
			(?:
				\s?[-—]\s?
				|\s[au]nd\s
			)
			[^\W\d_]+
		)*
	)
)??

(?P<suffix>
	(?:\s
		(?:
			m{,3}c?[dm]?c{,3}x?[lc]?x{,3}i?v?i{,3}
			|[js]r
		)
	)?
)
$
